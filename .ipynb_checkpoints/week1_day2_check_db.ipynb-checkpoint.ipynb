    "import pandas as pd\n",
    "from sqlalchemy import create_engine\n",
    "\n",
    "# Connect to SQLite DB\n",
    "engine = create_engine(\"sqlite:///week1/sales_db.sqlite\")\n",
    "\n",
    "# Read back the table\n",
    "df_sql = pd.read_sql(\"SELECT * FROM sales\", engine)\n",
